import java.util.*;
class Main {
    public  static void sol(int a,int d){
    int c=a+d;
    System.out.println(c);
} 
    public static void main(String[] args) {
        int a;
        int b;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter 2 numbers");
        a=sc.nextInt();
        b=sc.nextInt();
        sol(a,b);
    }
}
